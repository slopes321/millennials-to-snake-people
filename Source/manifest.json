{
	"manifest_version": 1,
	"name": "TweetSpeak Translator",
  "short_name": "TweetSpeak",
	"description": "Auto-translates TweetSpeak words directly on the web page",
	"author": "Silvio Lopes",
	"version": "1.0",
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": [
				"dictionary.js",
				"content_script.js"
			],
			"run_at": "document_end"
		}
	]
}
